{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
     "endOfLine": "auto"
   }],
    "linebreak-style": 0
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base/legacy"
  ]
}
